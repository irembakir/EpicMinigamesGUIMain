local function MDCVH_fake_script() -- Dane.LocalScript 
	local script = Instance.new('LocalScript', Dane)

	local player = game.Players.LocalPlayer
	local npc = game.Workspace:WaitForChild("Lobby"):WaitForChild("QuestStuff"):WaitForChild("NPCs"):WaitForChild("Dane"):WaitForChild("HumanoidRootPart")
	
	local button = script.Parent
	
	-- Function to teleport the player to the NPC
	local function teleportToNPC()
		local character = player.Character
		if character then
			local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
			if humanoidRootPart then
				-- Set the player's position to the NPC's position
				humanoidRootPart.CFrame = CFrame.new(npc.Position + Vector3.new(0, 3, 0)) -- Adjust to teleport above NPC
			end
		end
	end
	
	-- Button click event: Teleport the player to the NPC
	button.MouseButton1Click:Connect(function()
		teleportToNPC()
		print("Player Teleported to the EasterBunny NPC")
	end)
	
end
coroutine.wrap(MDCVH_fake_script)()
local function CTTMRAJ_fake_script() -- GetAllEggs.LocalScript 
	local script = Instance.new('LocalScript', GetAllEggs)

	local player = game.Players.LocalPlayer
	local button = script.Parent
	local eggsFolder = game.Workspace:WaitForChild("Stuff")
	
	-- Function to teleport the player to a random part inside each egg
	local function teleportToEggs()
		-- Get all eggs in the folder
		local eggs = {}
		for _, egg in pairs(eggsFolder:GetChildren()) do
			if egg.Name == "_ItemSmall" then
				table.insert(eggs, egg)
				print("Found egg model: " .. egg.Name)
			end
		end
	
		-- If no eggs were found
		if #eggs == 0 then
			print("No eggs named '_ItemSmall' found!")
			return
		end
	
		-- Loop through each egg and teleport player to a random part inside it with a delay
		for _, egg in ipairs(eggs) do
			-- Wait for 2 seconds before teleporting to the next egg
			wait(2)
	
			-- Debug: Print the current egg's name
			print("Teleporting to egg: " .. egg.Name)
	
			-- Find all the parts inside the egg model
			local eggParts = {}
			for _, part in pairs(egg:GetChildren()) do
				-- Debugging: Check if the part is a valid BasePart
				if part:IsA("BasePart") then
					table.insert(eggParts, part)
					print(part.Name .. " is a valid part inside " .. egg.Name)
				else
					print(part.Name .. " is not a valid part inside " .. egg.Name)
				end
			end
	
			-- If there are any valid parts, pick a random one
			if #eggParts > 0 then
				local randomPart = eggParts[math.random(1, #eggParts)]  -- Pick a random part
	
				-- Debugging: Print the part that was chosen
				print("Teleporting to random part: " .. randomPart.Name)
	
				-- Teleport player to the random part's position, just above it
				player.Character:SetPrimaryPartCFrame(randomPart.CFrame + Vector3.new(0, 5, 0))  -- Adjust height if needed
				print("Teleported to random part of egg: " .. egg.Name)
			else
				print("No valid parts found in egg: " .. egg.Name)
			end
		end
	end
	
	-- Button click event: Start teleporting to eggs
	button.MouseButton1Click:Connect(function()
		print("Button clicked. Starting teleportation process...")
		teleportToEggs()
		print("Teleportation process finished.")
	end)
end
coroutine.wrap(CTTMRAJ_fake_script)()
local function PJWKGV_fake_script() -- TextButton.Dragify 
	local script = Instance.new('LocalScript', TextButton)

	local player = game.Players.LocalPlayer
	
	local button = script.Parent
	
	local EasterEventFrame = script.Parent.Parent.Parent.EasterEvent
	local MenuFrame = script.Parent.Parent.Parent.Menu
	
	button.MouseButton1Click:Connect(function()
		-- Toggle EasterEventFrame visibility
		if EasterEventFrame.Visible == false then
			EasterEventFrame.Visible = true
			button.Text = "CLOSE"
		else
			EasterEventFrame.Visible = false
			button.Text = "OPEN"
		end
	
		-- Toggle MenuFrame visibility (optional if you want to toggle it too)
		if MenuFrame.Visible == false then
			MenuFrame.Visible = true
		else
			MenuFrame.Visible = false
		end
	end)
	
	
	
	local UIS = game:GetService("UserInputService")
	function dragify(Frame)
		dragToggle = nil
		dragSpeed = 0.15
		dragInput = nil
		dragStart = nil
		dragPos = nil
		function updateInput(input)
			Delta = input.Position - dragStart
			Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
			game:GetService("TweenService"):Create(Frame, TweenInfo.new(0.15), {Position = Position}):Play()
		end
		Frame.InputBegan:Connect(function(input)
			if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and UIS:GetFocusedTextBox() == nil then
				dragToggle = true
				dragStart = input.Position
				startPos = Frame.Position
				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragToggle = false
					end
				end)
			end
		end)
		Frame.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
				dragInput = input
			end
		end)
		game:GetService("UserInputService").InputChanged:Connect(function(input)
			if input == dragInput and dragToggle then
				updateInput(input)
			end
		end)
	end
	dragify(script.Parent)
	
end
coroutine.wrap(PJWKGV_fake_script)()
