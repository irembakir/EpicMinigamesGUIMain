-- Scripts:

local function HBNE_fake_script() -- MainEMMenu.LocalScript 
	local script = Instance.new('LocalScript', MainEMMenu)

	script.Parent.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/SlamminPig/rblxgames/main/Epic%20Minigames/EpicMinigamesGUI"))()
	end)
end
coroutine.wrap(HBNE_fake_script)()
local function RADRXAQ_fake_script() -- InfJump.LocalScript 
	local script = Instance.new('LocalScript', InfJump)

	--// Services
	local player = game.Players.LocalPlayer
	local uis = game:GetService("UserInputService")
	local debris = game:GetService("Debris")
	
	--// Button Setup
	local button = script.Parent
	local infJumpEnabled = false
	local holdingSpace = false
	
	--// Preserve the state across respawns
	local function saveInfJumpState(state)
		-- Store the state in player
		player:SetAttribute("InfJumpEnabled", state)
	end
	
	local function loadInfJumpState()
		-- Retrieve the state when respawned
		return player:GetAttribute("InfJumpEnabled") or false
	end
	
	--// Spawn landing part
	local function spawnLandingPart()
		if not player.Character or not player.Character:FindFirstChild("HumanoidRootPart") then return end
	
		local hrp = player.Character.HumanoidRootPart
	
		local landingPart = Instance.new("Part")
		landingPart.Size = Vector3.new(5, 1, 5)
		landingPart.Anchored = true
		landingPart.CanCollide = true
		landingPart.Transparency = 1 -- Invisible
		landingPart.Position = hrp.Position - Vector3.new(0, 5, 0)
		landingPart.Parent = workspace
	
		debris:AddItem(landingPart, 0.1)
	end
	
	--// Input Began
	local function onInputBegan(input, gameProcessed)
		if not infJumpEnabled then return end
		if input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode == Enum.KeyCode.Space then
			holdingSpace = true
			while holdingSpace and infJumpEnabled do
				spawnLandingPart()
				task.wait(0.05)
			end
		end
	end
	
	--// Input Ended
	local function onInputEnded(input, gameProcessed)
		if input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode == Enum.KeyCode.Space then
			holdingSpace = false
		end
	end
	
	--// Toggle Button
	button.MouseButton1Click:Connect(function()
		infJumpEnabled = not infJumpEnabled
		saveInfJumpState(infJumpEnabled)
		button.Text = "InfiniteJump: " .. (infJumpEnabled and "ON" or "OFF")
	end)
	
	--// Character Respawn Handling
	player.CharacterAdded:Connect(function()
		-- Load state after respawn
		infJumpEnabled = loadInfJumpState()
		button.Text = "InfiniteJump: " .. (infJumpEnabled and "ON" or "OFF")
	end)
	
	--// Connect Inputs
	uis.InputBegan:Connect(onInputBegan)
	uis.InputEnded:Connect(onInputEnded)
	
	--// Initialize button text with saved state (if any)
	infJumpEnabled = loadInfJumpState()
	button.Text = "InfiniteJump: " .. (infJumpEnabled and "ON" or "OFF")
	
end
coroutine.wrap(RADRXAQ_fake_script)()
local function JIWTNLO_fake_script() -- InfYield.LocalScript 
	local script = Instance.new('LocalScript', InfYield)

	script.Parent.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
	end)
end
coroutine.wrap(JIWTNLO_fake_script)()
local function BGLIF_fake_script() -- Lobby.LocalScript 
	local script = Instance.new('LocalScript', Lobby)

	local player = game.Players.LocalPlayer
	local button = script.Parent
	
	-- Function to kill the player
	local function killPlayer()
		local character = player.Character
		if character then
			local humanoid = character:FindFirstChildOfClass("Humanoid")
			if humanoid then
				humanoid.Health = 0  -- Set health to 0 to kill the player
			end
		end
	end
	
	-- Button click event: Kill the player
	button.MouseButton1Click:Connect(function()
		killPlayer()
		print("Player has been killed.")
	end)
	
end
coroutine.wrap(BGLIF_fake_script)()
local function ZJDT_fake_script() -- GoblinsHunter.LocalScript 
	local script = Instance.new('LocalScript', GoblinsHunter)

	--// Services
	local player = game.Players.LocalPlayer
	local button = script.Parent  -- Button for toggling
	local soulsFolder = game.Workspace:WaitForChild("Lobby"):WaitForChild("Souls")
	local userInputService = game:GetService("UserInputService")
	
	--// Variables
	local currentNpcIndex = 1  -- Keep track of the current NPC to teleport to
	local npcs = {}  -- List of NPCs
	local teleportDistance = 3  -- Distance from NPC to teleport player (closer to NPC)
	
	--// Function to get all the NPCs in the SoulsFolder
	local function getNpcs()
	    npcs = {}
	    for _, npc in ipairs(soulsFolder:GetChildren()) do
	        if npc.Name == "RockGoblin" and npc:IsA("Model") then
	            table.insert(npcs, npc)
	        end
	    end
	end
	
	--// Function to teleport player behind NPC
	local function teleportToNpc(npc)
	    if npc and npc.Parent then
	        local npcPosition = npc.PrimaryPart.Position  -- Get the position of the NPC's primary part
	        local behindNpcPosition = npcPosition - (npc.PrimaryPart.CFrame.LookVector * teleportDistance)  -- Position behind NPC
	        
	        -- Teleport the player to the position behind the NPC
	        player.Character:SetPrimaryPartCFrame(CFrame.new(behindNpcPosition))
	    end
	end
	
	--// Button press functionality
	button.MouseButton1Click:Connect(function()
	    getNpcs()  -- Populate the NPCs list
	
	    if currentNpcIndex <= #npcs then
	        local npc = npcs[currentNpcIndex]
	        if npc and npc.Parent then
	            teleportToNpc(npc)  -- Teleport the player behind the current NPC
	            currentNpcIndex = currentNpcIndex + 1  -- Move to the next NPC in the list
	        end
	    else
	        -- All NPCs have been visited, reset the index to start from the first NPC again
	        currentNpcIndex = 1
	        print("All NPCs visited, starting over!")
	    end
	end)
	
end
coroutine.wrap(ZJDT_fake_script)()
local function LOGHL_fake_script() -- EasterBunny.LocalScript 
	local script = Instance.new('LocalScript', EasterBunny)

	local player = game.Players.LocalPlayer
	local npc = game.Workspace:WaitForChild("Lobby"):WaitForChild("QuestStuff"):WaitForChild("NPCs"):WaitForChild("Easter Bunny"):WaitForChild("HumanoidRootPart")
	
	local button = script.Parent
	
	-- Function to teleport the player to the NPC
	local function teleportToNPC()
		local character = player.Character
		if character then
			local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
			if humanoidRootPart then
				-- Set the player's position to the NPC's position
				humanoidRootPart.CFrame = CFrame.new(npc.Position + Vector3.new(0, 3, 0)) -- Adjust to teleport above NPC
			end
		end
	end
	
	-- Button click event: Teleport the player to the NPC
	button.MouseButton1Click:Connect(function()
		teleportToNPC()
		print("Player Teleported to the EasterBunny NPC")
	end)
	
end
coroutine.wrap(LOGHL_fake_script)()
local function JWHDO_fake_script() -- EggVendingMachine.LocalScript 
	local script = Instance.new('LocalScript', EggVendingMachine)

	local player = game.Players.LocalPlayer
	local part = game.Workspace.Lobby.QuestStuff.Sets.eggVendingMachine.cannon:WaitForChild("FirePart")
	
	local button = script.Parent
	
	-- Function to teleport the player on top of the part
	local function teleportPlayer()
		local character = player.Character
		if character then
			-- Wait for the character's humanoid root part
			local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
			local humanoid = character:WaitForChild("Humanoid")
	
			-- Ensure the Firepart exists
			if part then
				-- Calculate the position on top of the part
				local newPosition = part.Position + Vector3.new(0, part.Size.Y / 2 + humanoid.HipHeight, 0)
	
				-- Teleport player to the new position
				humanoidRootPart.CFrame = CFrame.new(newPosition)
				print("Player Teleported to the top of Firepart")
			else
				warn("Firepart not found")
			end
		else
			warn("Character not loaded")
		end
	end
	
	-- Button click event: Teleport the player
	button.MouseButton1Click:Connect(function()
		teleportPlayer()
	end)
	
end
coroutine.wrap(JWHDO_fake_script)()
